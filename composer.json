{
    "name": "vanilla/garden-container",
    "description": "A dependency injection container.",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Burry",
            "email": "todd@vanillaforums.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "ext-xdebug": "*",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Garden\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garden\\Container\\Tests\\": "tests"
        }
    }
}
